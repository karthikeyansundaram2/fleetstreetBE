{
  "name": "fleetstreetbe",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "babel . --ignore node_modules -d dist",
    "build-start": "rm -rf dist && npm run build && nodemon dist/server.js",
    "transpile-views": "babel ./app/views -x \".jsx\" --out-dir ./dist/app/views",
    "copy-views": "babel ./app/views --out-dir ./dist/app/views --copy-files",
    "start": "nodemon --nolazy --exec babel-node server.js",
    "inspect": "nodemon --nolazy --exec babel-node --inspect-brk server.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "async": "^3.1.0",
    "aws-sdk": "^2.520.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^1.7.1",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "express-validator": "^6.2.0",
    "firebase-admin": "^5.12.1",
    "hashids": "^2.0.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "json2csv": "^4.5.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mysql2": "^1.7.0",
    "request": "^2.88.0",
    "sequelize": "^5.17.2",
    "uniqid": "^5.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "nodemon": "^1.19.1"
  }
}
